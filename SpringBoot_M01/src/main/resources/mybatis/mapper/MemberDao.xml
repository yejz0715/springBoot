<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
    <mapper namespace="com.ezen.spm01.dao.IProductDao">
    	
    	<select id="getMember" resultType="com.ezen.spm01.dto.MemberVO">
    	select * from member where id=#{param1}
    	</select>
    	
    	<select id="selectAddressByDong" resultType="com.ezen.spm01.dto.AdressVO">
    	select * from address where dong like '%'||#{param1}||'%'
    	</select>
    </mapper>
    